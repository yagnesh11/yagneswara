#include<stdio.h>
int main()
{
            int i,n[5];
            printf("Enter the digit to be reversed :\n");
            for(i=0;i<5;i++)
            {
                    scanf("%d",&n[i]);     
            }
            printf("reversed elements of array are :\n");
              for(i=4;i>=0;i--)
            {
                    printf("%d\n",n[i]);     
            }
      
}
