#include<stdio.h>
 
int main()
{
    int rows, cols , i, j;
     
    printf("Enter rows and columns of rectangle :");
    scanf("%d %d", &rows, &cols);
     
 
    for(i = 0; i < rows; i++)
    {
    
        for(j = 0; j < cols; j++)
        {
           printf("*");
        }
        printf("\n");
    }
    return 0;
}
