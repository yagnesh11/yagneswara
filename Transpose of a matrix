#include<stdio.h>
int main ()
{
     int a[10][10],c[10][10],n,i,j;
     printf("enter the value of n (n =< 10) : ");
     scanf("%d",&n);
     printf("Enter the value of matrix-a :\n");
     for(i=0;i<n;i++ )
     {
          for( j=0;j<n;j++)
          {
               scanf("%d",&a[i][j]);
          } 
     }
   printf("Transpose of the matrix :\n");
     for(i=0;i<n;i++)
     {
          for(j=0;j<n;j++)
          {
               c[i][j] = a[j][i] ;
               printf("%d\t",c[i][j]);
          }
          printf("\n");
     }
     return 0;
}
