#include<stdio.h>
int main()
{
    int i,j,k,n=5,c=1;
    for(i=0;i<=n;i++)
    {
        for(k=1;k<=n-i;k++)
        {
            printf(" ");
        }
        for(j=0;j<=i;j++)
        {
            if(i==0||j==0)
            c=1;
            else
            c=c*(i-j+1)/j;
            printf("%d ",c);
        }
        printf("\n");
    }
    return 0;
}
