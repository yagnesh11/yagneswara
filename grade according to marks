#include<stdio.h>
int main()
{
     int marks;
     printf("Enter the marks obtained = ");
     scanf("%d",&marks);
     if ( marks < 50 )
     {
          printf("Grade is F\n");
     }
     else if ( marks >= 50 ,marks < 60 )
     {
          printf("Grade is C\n");
     }
     else if ( marks >=60 ,marks < 70 )
     {
          printf("Grade is B\n");
     }
     else if ( marks >= 70, marks < 80 )
     {
          printf(" Grade is B+\n");
     }
     else if ( marks
      >= 80,marks < 90)
     {
          printf("Grade is A\n");
     }
     else 
     {
          printf("Grade is A+\n");
     }
}
