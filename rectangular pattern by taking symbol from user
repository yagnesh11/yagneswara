#include<stdio.h>
 
int main()
{
    int rows, cols , i, j;
    char x;
    printf("Enter symbol to be used in pattern :");
    scanf("%c",&x);
    printf("Enter rows and columns of rectangle :");
    scanf("%d %d", &rows, &cols);
 
    for(i = 0; i < rows; i++)
    {
    
        for(j = 0; j < cols; j++)
        {
           printf("%c",x);
        }
        printf("\n");
    }
    return 0;
}
