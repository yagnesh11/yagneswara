#include <stdio.h>
int main()
{
     int num[100],i,n;
     printf("Enter the number of values :");
     scanf("%d",&n);
     for(i=0;i<n;i++)
     {
          printf("Enter number %d :",i+1);
          scanf("%d",&num[i]);
     }
     for(i=0;i<n;i++)
     {
          printf(" %d \n",num[i]);
     
     }
     return 0;
     
}
