#include <stdio.h>

int main() {
   int num[100],i,n, smallest;
   
  printf("Enter number of elements: ");
  scanf("%d", &n);
   
   for(i= 0; i < n; ++i)
    {
         printf("Enter number %d :",i+1);
         scanf("%d",&num[i]);
      if( smallest > num[i] ) 
         smallest = num[i];
   }
   
   printf("Smallest element of array is %d", smallest);   
   
   return 0;
}
